{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServerInfo": {
    "Name": "ScreepsDotNet",
    "Build": "0.0.1-dev",
    "CliEnabled": false
  },
  "Storage": {
    "MongoRedis": {
      "MongoConnectionString": "mongodb://localhost:27017",
      "MongoDatabase": "screeps",
      "ServerInfoCollection": "serverData",
      "ServerInfoDocumentId": "serverInfo",
      "RedisConnectionString": "localhost:16379"
    }
  }
}
